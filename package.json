{
  "name": "@jdion/web-configs",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@latest",
  "author": "Jonathan Dion <jonathandionalary@gmail.com> (https://github.com/jonathandion/)",
  "license": "MIT",
  "scripts": {
    "audit": "pnpm audit --audit-level high",
    "build": "turbo run build --color --concurrency 14",
    "dev": "turbo run dev --color --concurrency 14",
    "change": "changeset",
    "cleanup": "turbo cleanup --concurrency 14",
    "ci": "turbo run build types:check lint audit --color --concurrency 14",
    "ci:debug": "act pull_request --secret-file my.secrets -s GITHUB_TOKEN=\"$(gh auth token)\" --container-architecture linux/amd64",
    "ci:publish": "pnpm publish -r",
    "ci:version": "changeset version && pnpm install --lockfile-only",
    "lint": "eslint ./ --max-warnings=0",
    "lint:debug": "eslint . --debug",
    "generate:types": "ts-node --esm --project tsconfig.node.json ./scripts/generate2.ts",
    "lint:fix": "pnpm lint --fix"
  },
  "dependencies": {
    "@jdion/cast": "workspace:*"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@jdion/eslint-config": "workspace:*",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.82",
    "@types/jest": "^29.5.4",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "turbo": "^1.10.13",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  }
}
