{
  "name": "@jdion/web-configs",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@latest",
  "author": "Jonathan Dion <jonathandionalary@gmail.com> (https://github.com/jonathandion/)",
  "license": "MIT",
  "scripts": {
    "audit": "pnpm audit",
    "build": "pnpm -r run build",
    "ci": "pnpm build && pnpm lint && pnpm audit",
    "ci:debug": "act --secret-file my.secrets",
    "ci:publish": "pnpm publish -r",
    "ci:version": "pnpm install --lockfile-only && changeset version",
    "lint": "eslint .",
    "lint:debug": "eslint . --debug",
    "lint:fix": "eslint . --fix",
    "lint:projects": "pnpm --filter=*-project run lint"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@jdion/eslint-config": "workspace:*",
    "eslint": "^8.35.0",
    "typescript": "^5.0.0"
  }
}
