{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**/*"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "coverage/**/*"
      ]
    },
    "types:check": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "cleanup": {
      "dependsOn": [],
      "outputs": []
    },
    "//#lint": {
      "dependsOn": [],
      "outputs": []
    },
    "//#audit": {
      "dependsOn": [],
      "inputs": [
        "pnpm-lock.yaml"
      ],
      "outputs": []
    }
  }
}
